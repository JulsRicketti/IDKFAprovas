package models;
 
import java.util.*;
import javax.persistence.*;
 
import play.db.jpa.*;
 
@Entity
public class Professor extends Model {
 
    public String nome;
    public String matricula;
    public String senha;
    public String formacao;
    public Date dataNascimento;
    public String email;
    
    public Professor(String nome, String senha, String matricula, Date dataNascimento, String formacao, String email) {
        this.nome = nome;
        this.senha = senha;
        this.matricula = matricula;
        this.dataNascimento = dataNascimento;
        this.formacao = formacao;
        this.email = email;
    }
 
}
